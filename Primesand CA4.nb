Cell[BoxData[
 RowBox[{
  RowBox[{"PrimeDifferences", "[", "n_", "]"}], ":=", 
  RowBox[{"NestList", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{"Differences", "[", "#", "]"}], "]"}], "&"}], ",", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Prime", "[", "i", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", 
    RowBox[{"n", "-", "1"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.700774287998974*^9, 3.700774297728869*^9}, {
   3.7051045527899556`*^9, 3.7051045647306385`*^9}, {3.705105385992612*^9, 
   3.7051053864506383`*^9}, {3.7070420580239506`*^9, 3.70704210073841*^9}, 
   3.707042239883752*^9},
 CellLabel->"In[2]:=",ExpressionUUID->"6f1323c1-6aca-4f37-9112-fe04ada66d09"],

